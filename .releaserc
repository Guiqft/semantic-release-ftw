{
    "branches": [
        "+([0-9])?(.{+([0-9]),x}).x",
        "main",
        {
            "name": "dev",
            "prerelease": true
        },
        {
            "name": "homolog",
            "prerelease": "rc"
        }
    ],
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        [
            "@semantic-release/git",
            {
                "assets": [
                    "package.json"
                ]
            }
        ],
        [
            "@semantic-release/github",
            {
                "assets": [
                    {
                        "path": "dist/**"
                    }
                ],
                "draftRelease": true
            }
        ]
    ]
}
